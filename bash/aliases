# cd
alias ..='cd ..'

# ls
alias ls='ls -F'
alias l='ls -lAh'
alias ll='ls -l'
alias la='ls -A'

# git
alias gl='git pull'
alias gp='git push'
alias gd='git diff'
alias gc='git commit'
alias gca='git commit -a'
alias gco='git checkout'
alias gb='git branch'
alias gs='git status'
alias gcl='git clone'

alias c='clear'

alias update-grub-fedora='grub2-mkconfig -o /boot/grub2/grub.cfg'
